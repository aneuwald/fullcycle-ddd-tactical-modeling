{
	"extends": [
		"airbnb"
	],
	"plugins": [
		"@typescript-eslint"
	],
	"parser": "@typescript-eslint/parser",
	"rules": {
		"import/no-unresolved": 0,
		"semicolon": [true, "always"],
		"semi": [true, "always"],
		"react/jsx-filename-extension": {
			"extensions": [
				".jsx",
				".tsx"
			]
		}
	}
}